# Environment Variables Example
# Copy this file to .env and fill in your actual values

# Network Configuration
VITE_NETWORK_NAME=lineaTestnet
VITE_RPC_URL=https://rpc.linea.build

# Contract Addresses (update after deployment)
VITE_TRUSTLINK_CORE_ADDRESS=0x...
VITE_TRUSTLINK_REGISTRY_ADDRESS=0x...
VITE_PROOF_VERIFIER_ADDRESS=0x...

# Application Configuration
VITE_APP_NAME=TrustLink
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_MAINTENANCE_MODE=false